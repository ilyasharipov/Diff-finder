{
    "name": "yanepenb/gendiff",
    "description": "Utility for finding differences in configuration files.",
    "keywords": ["gendiff", "cli", "parser"],
    "type": "project",
    "authors": [
        {
            "name": "Ilya Sharipov",
            "email": "eanepenb@mail.ru"
        }
    ],
    "scripts": {
        "phpcs": "phpcs",
	    "phpunit": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "DiffFinder\\Tests\\": "tests"
        },
        "files": [
            "src/cli.php",
            "src/differ.php",
            "src/parser.php",
            "src/ast.php",
            "src/output.php",
            "src/format/plain.php",
            "src/format/json.php"
        ]
    },
    "bin": [
      "bin/gendiff"
    ],
    "require": {
        "docopt/docopt": "^1.0",
        "funct/funct": "^1.5",
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.4",
        "phpunit/phpunit": "^8.3",
	    "codeclimate/php-test-reporter": "dev-master"
    }
}
